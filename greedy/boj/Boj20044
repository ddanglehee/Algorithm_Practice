import kotlin.math.min

class Boj20044 {
    fun solution() {
        val n = readLine()!!.toInt()
        val wList = readLine()!!.split(" ").map { it.toInt() }.toMutableList()

        wList.sort()
        var result = wList[0] + wList[wList.lastIndex]

        for (i in 1 until n) {
            result = min(result, wList[i] + wList[2 * n - i - 1])
        }

        println(result)
    }
}